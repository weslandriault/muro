					<li><%= link_to "More Applications", applications_more_applications_path %></li>
				</ul>
			</li>

			<li class="customer-service"><%= link_to "CUSTOMER SERVICE", customer_service_path, :class=>"two-line" %>
				<ul>
					<li class="no-margin-top"><%= link_to "FAQs", customer_service_faqs_path %></li>
					<li><%= link_to "Product Manuals", customer_service_product_manuals_path %></li>
					<li><%= link_to "Parts List and Schematics", "#", :class=>"parts_and_schematics" %></li>
					<li><%= link_to "Product Registration", customer_service_product_registration_path %></li>
					<li><%= link_to "Testimonials", customer_service_testimonials_path %></li>
				</ul>
			</li>
			<li class="downloads"><%= link_to "DOWNLOADS", downloads_path %>
