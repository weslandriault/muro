					<li class="no-margin-top"><%= link_to "Brochures", downloads_brochures_path %></li>
					<li><%= link_to "Articles", downloads_articles_path %></li>
					<li><%= link_to "Videos", downloads_videos_path %></li>
				</ul>
			</li>
		</ul>
	</nav>
</div>

	<div id="parts_and_schematics_popup">
	  <div class="close"></div>

    <h3 class="service_portal">Muro North America Inc. Service Portal</h3>

    </p>

