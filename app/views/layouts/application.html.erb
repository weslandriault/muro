<div class="fixed">
	<header class="group">
		<div class="wrapper">

			<img class="muro-large" src="/muro-logo.png" alt="Muro North America" />

			<div class="right group">
				<nav class="top group">
					<div class="social-links">
						<a href="https://www.youtube.com/channel/UCV9IO9TKgZY3aADGA3abS4Q/videos"><img src="/youtube-header.png" alt="Muro Youtube page"></a>
						<a href="https://www.facebook.com/pages/Muro-North-America/269584249834668"><img src="/facebook-header.png" alt="Muro Facebook Page"></a>
					</div>

					<h2 style="display:none">THE ORIGINATORS OF AUTO-FEED <br>CONTINUE TO DRIVE INNOVATION</h2>

					<ul class="contact-findus">
						<li class="contact"><%= link_to "Contact", contact_path %></li>
						<li class="find-us"><%= link_to "Find Us", contact_find_us_path %></li>

					</ul>

					<div id="find-a-distributor">
						<ul>
							<li><%= link_to "Find a Distributor", distributors_path %>
								<ul>
									<li class="no-margin-top"><%= link_to "North America", distributors_north_america_path %></li>
									<li><%= link_to "International", distributors_international_path %></li>
									<li><%= link_to "Online", distributors_online_path %></li>
								</ul>
							</li>
						</ul>
					</div>

					<%= form_tag search_path, :class=> "search", method: :get do  %>
						<%= text_field_tag :q, nil, :placeholder=>"Search for a product" %>
						<%= submit_tag "Search" %>
					<% end %>
				</nav>
			</div>
		</div>
	</header>



	<nav class="main group">
		<ul class="wrapper group">
			<li class="home"><a href="/">HOME</a></li>

			<li class="about"><%= link_to "ABOUT", about_url %>
				<ul>
					<li><%= link_to "Muro North America", about_muro_north_america_path %></li>
